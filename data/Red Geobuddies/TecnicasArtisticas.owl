    <!-- http://www.semanticweb.org/ontologies/2008/03/TecnicasArtisticas.owl#acuarela -->

    <TecnicasArtisticas:TecnicaDePintura rdf:about="&TecnicasArtisticas;acuarela">
        <rdfs:label xml:lang="es">acuarela</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Use for watercolor painting done in transparent washes of color as distinguished from the opaque method using gouache.</rdfs:comment>
    </TecnicasArtisticas:TecnicaDePintura>
    


