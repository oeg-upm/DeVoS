ns4:MessagePriority a owl:NamedIndividual,
        ns4:Concern ;
    rdfs:label "Message precedence"@en ;
    ns7:hasStakeholder <https://www.wikidata.org/wiki/Q1548249>,
        <owl:Developer> ;
