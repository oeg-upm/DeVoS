@prefix ns1: <http://purl.org/dc/terms/> .
@prefix ns2: <http://www.w3.org/2004/02/skos/core#> .
@prefix ns3: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix ns4: <http://purl.org/vocab/vann/> .
@prefix ns5: <http://purl.org/ontology/bibo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:contributor a owl:AnnotationProperty .

ns1:created a owl:AnnotationProperty .

ns1:creator a owl:AnnotationProperty .

ns1:description a owl:AnnotationProperty .

ns1:license a owl:AnnotationProperty .

ns1:modified a owl:AnnotationProperty .

ns1:title a owl:AnnotationProperty .

ns5:status a owl:AnnotationProperty .

ns4:preferredNamespacePrefix a owl:AnnotationProperty .

ns4:preferredNamespaceUri a owl:AnnotationProperty .

ns3:term_status a owl:AnnotationProperty .

ns2:definition a owl:AnnotationProperty .

: a owl:Ontology ;
    ns1:contributor "Gottfried Schenner" ;
    ns1:created "2020-11-16" ;
    ns1:creator "Stefan Bischof <bischof.stefan@siemens.com>" ;
    ns1:description "The Rail Topology Ontology models foundational rail topology concepts.." ;
    ns1:license "Mozilla Public License, Version 2.0" ;
    ns1:modified "2021-04-20" ;
    ns1:title "Rail Topology Ontology" ;
    ns5:status "Published" ;
    ns4:preferredNamespacePrefix "topo" ;
    ns4:preferredNamespaceUri "https://w3id.org/rail/topo#" ;
    rdfs:seeAlso <https://www.railtopomodel.org/> ;
    owl:imports <http://standards.iso.org/iso/15926/part14/1.0> ;
    owl:versionIRI <https://w3id.org/rail/topo/v1.0#> ;
    owl:versionInfo "v1.0" .

:LinearElement a owl:Class ;
    rdfs:label "LinearElement" ;
    rdfs:subClassOf :PositioningNetElement ;
    owl:disjointWith :NonLinearElement ;
    ns2:definition "One-dimensional \"PositioningNetElement\" with a start (intrinsic coordinate 0.0) and an end (intrinsic coordinate 1.0)." .

:Orientation a rdfs:Datatype ;
    rdfs:label "Orientation" ;
    owl:equivalentClass [ a rdfs:Datatype ;
            owl:oneOf [ a rdf:List ;
                    rdf:first -1 ;
                    rdf:rest [ a rdf:List ;
                            rdf:first 1 ;
                            rdf:rest () ] ] ] .

:anchor a owl:AsymmetricProperty,
        owl:InverseFunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "anchor" ;
    rdfs:domain :LinearPositioningSystem ;
    rdfs:range :LinearAnchorPoint .

:anchorName a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "anchorName" ;
    rdfs:domain :LinearAnchorPoint ;
    rdfs:range rdfs:Literal ;
    ns2:definition "Name of the “LinearAnchorPoint” instance which is unique within the given \"LinearPositioningSystem\"" .

:crsDefininition a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "crsDefininition" ;
    rdfs:domain :GeometricPositioningSystem ;
    rdfs:range rdfs:Literal ;
    ns2:definition "coordinate reference system" .

:elementCollection a owl:AsymmetricProperty,
        owl:InverseFunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "elementCollection" ;
    rdfs:domain :CompositionNetElement ;
    rdfs:range :ElementPartCollection .

:elementPartList a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "elementPartList" ;
    rdfs:domain :OrderedCollection ;
    rdfs:range rdf:List .

:endMeasure a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "endMeasure" ;
    rdfs:domain :LinearPositioningSystem ;
    rdfs:range xsd:double ;
    ns2:definition "Value for measurement at the end of the \"LinearPositioningSystem\"" .

:id a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "id" ;
    rdfs:domain :BaseObject ;
    rdfs:range rdfs:Literal ;
    ns2:definition "unique identifier" .

:intrinsicCoord a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "intrinsicCoord" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :IntrinsicCoordinate :SpotLocationIntrinsic ) ] ;
    rdfs:range xsd:double ;
    ns2:definition "Location in reference to the chosen NetElement, given as value in the interval from 0 to 1." .

:intrinsicCoordBegin a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "intrinsicCoordBegin" ;
    rdfs:domain :AssociatedNetElementIntrinsic ;
    rdfs:range xsd:double ;
    ns2:definition "Start location of the \"NetEntity\" instance in relation to the PositioningNetElement\" which is used for positioning within the network." .

:intrinsicCoordEnd a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "intrinsicCoordEnd" ;
    rdfs:domain :AssociatedNetElementIntrinsic ;
    rdfs:range xsd:double ;
    ns2:definition "End location of the \"NetEntity\" instance in relation to the \"PositioningNetElement\" which is used for positioning within the network." .

:keepsOrientation a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "keepsOrientation" ;
    rdfs:domain :AssociatedNetElement ;
    rdfs:range xsd:boolean ;
    ns2:definition "child `LinearElement` keeps same Orientation as parent `LinearElement`. `false`: Orientation is not relevant, `true`: Orientation is relevant"@en .

:lateralOffset a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "lateralOffset" ;
    rdfs:domain :LinearCoordinate ;
    rdfs:range xsd:double ;
    ns2:definition "distance perpendicular to the \"line of reference\""@en .

:longname a owl:AnnotationProperty ;
    rdfs:label "longname" ;
    rdfs:domain :NamedResource ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf rdfs:comment ;
    ns2:definition "Longer name of a resource" .

:measure a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "measure" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :LinearAnchorPoint :LinearCoordinate ) ] ;
    rdfs:range xsd:double ;
    ns2:definition """For LinearCoordinate: location at the "line of reference" (possibly adjusted to local anomalies using "LinearAnchorPosition")
For LinearAnchorPoint: Measure of the Anchor Point within the given“LinearPositioningSystem”"""@en .

:measureToNext a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "measureToNext" ;
    rdfs:domain :LinearAnchorPoint ;
    rdfs:range xsd:double ;
    ns2:definition "Basis for modified interpolation of location in the interval up to the next “LinearAnchorPoint” of the given  \"LinearPositioningSystem”." .

:name a owl:AnnotationProperty ;
    rdfs:label "name" ;
    rdfs:domain :NamedResource ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf rdfs:label ;
    ns2:definition "short name of a resource" .

:networkResource a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "networkResource" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :LevelNetwork :Network ) ] ;
    rdfs:range :NetworkResource ;
    rdfs:subPropertyOf <http://standards.iso.org/iso/15926/part14/hasFunctionalPart> .

:reaches a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "reaches" ;
    rdfs:domain :IntrinsicCoordinate ;
    rdfs:range :IntrinsicCoordinate ;
    ns3:term_status "testing" ;
    ns2:definition "Asserts that one end of a <#LinearElement> given as subject <#IntrinsicCoordinate> can reach the object <#IntrinsicCoordinate> and its corresponding <#LinearElement> without change of direction" .

:sequence a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "sequence" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :OrderedAssociatedNetElement :OrderedCollection ) ] ;
    rdfs:range xsd:nonNegativeInteger ;
    ns2:definition "Sequence of the child element within the ordered collection" .

:startMeasure a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "startMeasure" ;
    rdfs:domain :LinearPositioningSystem ;
    rdfs:range xsd:double ;
    ns2:definition "Value for measurement at the beginning of the \"LinearPositioningSystem\"" .

:unit a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "unit" ;
    rdfs:domain :LinearPositioningSystem ;
    rdfs:range rdfs:Literal ;
    ns2:definition "unit for measurement" .

:validFrom a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "validFrom" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :AssociatedPositioningSystem :NetworkResource :PositioningSystem ) ] ;
    rdfs:range xsd:dateTime ;
    ns2:definition "Point in time where the object is available for usage for train operations (if empty, then the object is valid till the [validTo](#validTo) date)." .

:validTo a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "validTo" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :AssociatedPositioningSystem :NetworkResource :PositioningSystem ) ] ;
    rdfs:range xsd:dateTime ;
    ns2:definition "Point in time where the object is no longer available for functional usage (if empty, then the object is valid since the [validFrom](#validFrom) date)." .

:verticalOffset a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "verticalOffset" ;
    rdfs:domain :LinearCoordinate ;
    rdfs:range xsd:double ;
    ns2:definition "height above the \"line of reference\" at the position defined by \"measure\""@en .

:x a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "x" ;
    rdfs:domain :GeometricCoordinate ;
    rdfs:range xsd:double ;
    ns2:definition "x value of cartesian coordinate, longitude of spherical coordinate"@en .

:y a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "y" ;
    rdfs:domain :GeometricCoordinate ;
    rdfs:range xsd:double ;
    ns2:definition "y value of cartesian coordinate, latitude of spherical coordinate"@en .

:z a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "z" ;
    rdfs:domain :GeometricCoordinate ;
    rdfs:range xsd:double ;
    ns2:definition "z value of cartesian coordinate, altitude of spherical coordinate"@en .

